public class Solution {

  public static ArrayList<Integer> getLevelOrder(BinaryTreeNode root) {
    //Your code goes here
	ArrayList<Integer> ans=new ArrayList<>();
	if(root==null){
		return ans;
	}
	Queue<BinaryTreeNode> q=new LinkedList<>();
	q.add(root);

	while(!q.isEmpty()){
		BinaryTreeNode curr=q.poll();
		ans.add(curr.val);
		if(curr.left!=null){
			q.add(curr.left);
		}
		if(curr.right!=null){
			q.add(curr.right);
		}
	}
	return ans;

  }

}
TC:O(N)
SC:O(N)
